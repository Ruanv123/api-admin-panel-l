generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Dados {
  id             Int     @id @default(autoincrement())
  urlLogo        String
  nomeApp        String  @unique
  linkCheckout   String
  codAfiliado    String  @unique
  linkBanner1    String?
  linkBanner2    String?
  linkBanner3    String?
  linkTwitch     String
  linkComunidade String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
